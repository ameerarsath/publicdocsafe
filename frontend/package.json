{
  "name": "securevault-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "SecureVault - Enterprise-grade secure document storage for small teams",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3005",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "quality:check": "npm run type-check && npm run lint && npm run format",
    "deps:check": "npx depcheck"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.0",
    "@types/node": "^24.5.2",
    "axios": "^1.3.0",
    "clsx": "^1.2.0",
    "docx-preview": "^0.3.6",
    "jszip": "^3.10.1",
    "lucide-react": "^0.263.0",
    "mammoth": "^1.11.0",
    "pdfjs-dist": "^5.4.149",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.43.0",
    "react-pdf": "^10.1.0",
    "react-router-dom": "^6.8.0",
    "tailwind-merge": "^1.10.0",
    "xlsx": "^0.18.5",
    "zod": "^3.20.0",
    "zustand": "^4.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
